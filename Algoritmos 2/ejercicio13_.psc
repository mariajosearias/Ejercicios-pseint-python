Algoritmo Inicio_Numero_Billetes
	Escribir 'Calcularemos cuanto dinero hay en un banco'
	N1 <- 50000*1
	N2 <- 20000*2
	N3 <- 10000*3
	N4 <- 5000*4
	N5 <- 2000*5
	N6 <- 1000*6
	N7 <- 500*7
	N8 <- 100*8
	Valortotal <- N1+N2+N3+N4+N5+N6+N7+N8
	Escribir 'La cantidad de dinero que hay en el banco es $',Valortotal
FinAlgoritmo
